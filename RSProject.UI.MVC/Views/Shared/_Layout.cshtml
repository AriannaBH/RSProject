        <ul class="main-menu">
            <li><a class="active" href="@Url.Action("Index", "Home")">Home</a></li>
            <li><a href="@Url.Action("Index", "Reservations")">Reservations</a></li>
            <li><a href="@Url.Action("Index", "Services")">Services</a></li>
